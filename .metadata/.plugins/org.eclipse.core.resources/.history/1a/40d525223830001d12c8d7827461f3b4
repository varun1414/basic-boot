package com.example.standards.entity;

import javax.persistence.Id;

public class Review {
	
	@Id
	int id;
	
	String comment;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getComment() {
		return comment;
	}

	public void setComment(String comment) {
		this.comment = comment;
	}
	
	
}
